{
  "name": "tenoxui",
  "version": "0.12.6",
  "description": "A CSS Framework without CSS file :D",
  "author": "NOuSantx <nousantx@gmail.com>",
  "license": "MIT",
  "homepage": "https://tenoxui.web.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenoxui/tenoxui.git",
    "directory": "packages/tenoxui"
  },
  "bugs": {
    "url": "https://github.com/tenoxui/tenoxui/issues"
  },
  "keywords": [
    "framework",
    "css",
    "css-in-js",
    "utility-first"
  ],
  "type": "module",
  "types": "./dist/js/tenoxui.d.ts",
  "main": "./dist/js/tenoxui.js",
  "module": "./dist/js/tenoxui.esm.js",
  "style": "./dist/css/tenoxui.css",
  "files": [
    "dist/*",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:css": "cleancss src/css/tenoxui.css -o dist/css/tenoxui.min.css --source-map && cp src/css/tenoxui.css dist/css/"
  },
  "devDependencies": {
    "@nousantx/someutils": "^0.3.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@tenoxui/core": "1.2.1",
    "@types/node": "^22.7.8",
    "clean-css-cli": "^5.6.3",
    "prettier": "^3.2.5",
    "rollup": "^4.24.0",
    "rollup-plugin-strip-banner": "^3.1.0",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "uglify-js": "^3.19.3"
  }
}
