{
  "name": "@tenoxui/property",
  "version": "1.4.3",
  "description": "TenoxUI type shorthands and properties library.",
  "author": "NOuSantx <nousantx@gmail.com>",
  "license": "MIT",
  "homepage": "https://tenoxui.web.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tenoxui/tenoxui.git",
    "directory": "packages/@tenoxui-property"
  },
  "bugs": {
    "url": "https://github.com/tenoxui/tenoxui/issues"
  },
  "keywords": [
    "css",
    "css-framework",
    "css-in-js"
  ],
  "type": "module",
  "types": "./dist/default.d.ts",
  "main": "./dist/default.cjs",
  "module": "./dist/default.esm.js",
  "browser": "./dist/default.umd.min.js",
  "exports": {
    ".": {
      "types": "./dist/default.d.ts",
      "import": "./dist/default.esm.js",
      "require": "./dist/default.cjs",
      "browser": "./dist/default.umd.min.js"
    },
    "./full": {
      "types": "./dist/full.d.ts",
      "import": "./dist/full.esm.js",
      "require": "./dist/full.cjs",
      "browser": "./dist/full.umd.min.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@nousantx/someutils": "^0.3.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.7.8",
    "rollup": "^4.24.0",
    "terser": "^5.31.6",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  }
}
